<p id="notice"><%= notice %></p>

<h1>Listing Participant Flows</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @participant_flows.each do |participant_flow| %>
      <tr>
        <td><%= link_to 'Show', participant_flow %></td>
        <td><%= link_to 'Edit', edit_participant_flow_path(participant_flow) %></td>
        <td><%= link_to 'Destroy', participant_flow, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Participant flow', new_participant_flow_path %>
